@model Vidly.Models.Customer
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>@Model.Name</h2>
<table>
    <thead>
        <tr>
            <td>Membership Type : @Model.MembershipType.Name</td>
        </tr>
        @if (@Model.Birthdate != null)
        {
            <tr>
                <td>
                    Birthdate :
                    @Model.Birthdate
                </td>
            </tr>
        }
    </thead>
</table>


